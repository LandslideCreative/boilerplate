{
    "name": "landslidecreative/ls-boilerplate-wp",
    "authors": [
        {
            "name": "Landslide Creative",
            "email": "info@landslidecreative.com"
        }
    ],
    "repositories" : [
        {
            "type" : "composer",
            "url"  : "https://wpackagist.org"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:jsjenkins/advanced-custom-fields-pro.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:jsjenkins/gravityforms.git"
        },
        
        {
           "type" : "vcs",
           "url" : "git@github.com:jsjenkins/events-calendar-pro.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:jsjenkins/wp-migrate-db-pro.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:jsjenkins/wp-rocket.git"
        }
    ],
    "extra" : {
        "installer-paths" : {
            "wp-content/plugins/{$name}/" : ["type:wordpress-plugin"],
            "wp-content/themes/{$name}/" : ["type:wordpress-theme"]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "landslidecreative/advanced-custom-fields-pro" : "6.2.6",
        "wpackagist-plugin/akismet" : "5.3.1",
        "wpackagist-plugin/classic-editor" : "1.6.3",
        "wpackagist-plugin/duplicate-post" : "4.5",
        "landslidecreative/gravityforms" : "2.8.3",
        "wpackagist-plugin/imagify" : "2.1.3.1",
        "wpackagist-plugin/intuitive-custom-post-order" : "3.1.5",
        "wpackagist-plugin/query-monitor" : "3.15",
        "wpackagist-plugin/safe-redirect-manager" : "2.1.1",
        "wpackagist-plugin/svg-support" : "2.5.5",
        "wpackagist-plugin/the-events-calendar" : "6.3.0",
        "landslidecreative/events-calendar-pro" : "6.3.0",
        "wpackagist-plugin/user-switching" : "1.7.3",
        "wpackagist-plugin/wordfence" : "7.11.3",
        "wpackagist-plugin/wp-crontrol" : "1.16.1",
        "landslidecreative/wp-migrate-db-pro" : "2.6.11",
        "landslidecreative/wp-rocket" : "3.15.6",
        "wpackagist-plugin/wordpress-seo" : "22.1",
        "wpackagist-plugin/yet-another-related-posts-plugin" : "5.30.10"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
