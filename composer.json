{
    "name": "landslidecreative/ls-boilerplate-wp",
    "authors": [
        {
            "name": "Landslide Creative",
            "email": "info@landslidecreative.com"
        }
    ],
    "repositories" : [
        {
            "type" : "composer",
            "url"  : "https://wpackagist.org"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:jsjenkins/advanced-custom-fields-pro.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:jsjenkins/gravityforms.git"
        },
        
        {
           "type" : "vcs",
           "url" : "git@github.com:jsjenkins/events-calendar-pro.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:jsjenkins/wp-migrate-db-pro.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:jsjenkins/wp-rocket.git"
        }
    ],
    "extra" : {
        "installer-paths" : {
            "wp-content/plugins/{$name}/" : ["type:wordpress-plugin"],
            "wp-content/themes/{$name}/" : ["type:wordpress-theme"]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "landslidecreative/advanced-custom-fields-pro" : "6.7.0",
        "wpackagist-plugin/akismet" : "5.6",
        "wpackagist-plugin/classic-editor" : "1.6.7",
        "landslidecreative/gravityforms" : "2.9.26",
        "wpackagist-plugin/imagify" : "2.2.7",
        "wpackagist-plugin/intuitive-custom-post-order" : "3.2.0",
        "wpackagist-plugin/query-monitor" : "3.20.2",
        "wpackagist-plugin/safe-redirect-manager" : "2.2.2",
        "wpackagist-plugin/svg-support" : "2.5.14",
        "wpackagist-plugin/the-events-calendar" : "6.15.15",
        "landslidecreative/events-calendar-pro" : "7.7.12",
        "wpackagist-plugin/user-switching" : "1.11.1",
        "wpackagist-plugin/wordfence" : "8.1.4",
        "wpackagist-plugin/wp-crontrol" : "1.21.0",
        "landslidecreative/wp-migrate-db-pro" : "2.7.7",
        "landslidecreative/wp-rocket" : "3.20.3",
        "wpackagist-plugin/yet-another-related-posts-plugin" : "5.30.11",
        "wpackagist-plugin/duplicate-post" : "4.5",
        "wpackagist-plugin/wordpress-seo" : "26.8"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
